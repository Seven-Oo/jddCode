oolname: Random.cname(),\r\n      visits: Random.natural(10000, 10000000),\r\n      desc: Random.csentence(10, 25),\r\n      courseUrl: Random.url(),\r\n    };\r\n\r\n    courserecommendList.push(get);\r\n  }\r\n\r\n  // 返回状态码和文数据\r\n  return {\r\n    code,\r\n    courserecommendList,\r\n  };\r\n};\r\n\r\n// 定义请求链接，类型，还有返回数据\r\nmock(`${domain}/course`, 'get', getData);\r\n\r\n//  post data\r\nconst postData = (req) => {\r\n  // console.log(req) // 请求体，用于获取参数\r\n\r\n  const slidersData = [];\r\n  for (let i = 0; i < 8; i++) {\r\n    const post = {\r\n      src: Random.dataImage('375x208', '课程封面'),\r\n      title1: Random.csentence(8, 16),\r\n      title2: Random.csentence(8, 16),\r\n      paragraphs: Random.csentence(10, 25),\r\n      href: Random.url(),\r\n      hrefTxt: '查看更多',\r\n    };\r\n\r\n    slidersData.push(post);\r\n  }\r\n  const slidersDataf = {\r\n    slideSpeed: 3000,\r\n    slides: [\r\n      {\r\n        src: '../assets/carousel-1.png',\r\n        title1: '一一一央媒省媒聚焦',\r\n        title2: '南通市域治理现代化',\r\n        paragraphs: `IDC最新发布报告显示，京东数科T1金融云进入金融云平台解\r\n        决方案市场前五名，在中国金融云（平台）解决方案市场上占据了9`,\r\n        href: 'https://www.jddglobal.com/',\r\n        hrefTxt: '查看更多',\r\n      },\r\n      {\r\n        src: '../assets/carousel-2.png',\r\n        title1: '二二二南通市域治理现代化',\r\n        paragraphs: `IDC最新发布报告显示，京东数科T1金融云进入金融云平台解\r\n        决方案市场前五名，在中国金融云（平台）解决方案市场上占据了9`,\r\n        href: 'https://www.jddglobal.com/',\r\n        hrefTxt: '查看更多',\r\n      },\r\n      {\r\n        src: '../assets/carousel-3.png',\r\n        title1: '三三三央媒省媒聚焦',\r\n        title2: '南通市域治理现代化',\r\n        paragraphs: `IDC最新发布报告显示，京东数科T1金融云进入金融云平台解\r\n        决方案市场前五名，在中国金融云（平台）解决方案市场上占据了9`,\r\n      },\r\n    ],\r\n  };\r\n\r\n  // 返回状态码和数据\r\n  return {\r\n    slidersData,\r\n  };\r\n};\r\nmock(`${domain}/sliders`, 'post', postData);\r\n"]}]}